{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "HackerNewsConfiguration": {
    "BaseAddress": "https://hacker-news.firebaseio.com/v0/",
    "AcquireTimeoutMs": 500,
    "PermitLimit": 50,
    "WindowSeconds": 1,
    "QueueLimit": 500,
    "HttpClientTimeoutSeconds": 30,
    "PollyBulkheadMaxParallelization": 20,
    "PollyBulkheadMaxQueuingActions": 500,
    "PollyRetryCount": 3,
    "PollyRetryJitterMsMax": 100
  },

  "CacheConfiguration": {
    "TtlInMs": 300000
  }
}
